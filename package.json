{
  "name": "app",
  "description": "App",
  "author": "NPT",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "ng:dev": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check",
    "ng:build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app:build": "npm run ng:build && npm run electron:serve-tsc",
    "electron": "electron .",
    "electron:serve-tsc": "tsc -p tsconfig-serve.electron.json",
    "electron:serve": "npm run app:electron:pre:build && npm run electron:serve-tsc && npm run electron",
    "app:electron:pre:build": "build.sh",
    "electron:builder": "electron-builder --publish never",
    "electron:builder:publish": "electron-builder --publish always"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<dev>/<app>.git"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/flex-layout": "~11.0.0-beta.33",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "cleave.js": "^1.6.0",
    "electron-log": "^4.3.2",
    "knex": "^0.21.2",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "ng-zorro-antd": "^11.2.0",
    "ngx-cleave-directive": "^1.1.3",
    "ngx-toastr": "^12.1.0",
    "objection": "^2.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/localize": "^11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.14.36",
    "codelyzer": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
